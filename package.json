{
  "name": "remix-docs-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build:posts": "npx ts-node scripts/cachePosts.ts",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:posts": "npx ts-node scripts/cachePosts.ts watch",
    "dev:remix": "remix dev",
    "postinstall": "npm run build:posts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@markdoc/markdoc": "^0.2.1",
    "@octokit/plugin-throttling": "^4.3.2",
    "@octokit/rest": "^19.0.5",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/vercel": "^1.9.0",
    "@vercel/node": "^2.8.2",
    "add": "^2.0.6",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "front-matter": "^4.0.2",
    "inflected": "^2.1.0",
    "isbot": "^3.6.5",
    "mdx-bundler": "^9.2.1",
    "p-queue": "^7.3.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "remix-seo": "^0.1.0",
    "remix-utils": "^5.1.0",
    "spin-delay": "^1.2.0",
